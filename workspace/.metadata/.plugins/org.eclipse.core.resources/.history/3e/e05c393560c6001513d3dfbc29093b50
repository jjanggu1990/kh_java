package java_0129;


class ExceptionEx {

	public static void main(String[] args) {
		int [] i ={10, 20, 30};
		
		for(int j = 0;j<i.length +1;j++){
			try{
			System.out.println(i[j]);
			}
			catch(Exception e){
				System.out.println(e.getMessage());
			}
		}
		
		System.out.println("program END");
		
	}

}
