class Car3{
	
	String color;
	String gearType;
	int door;

	Car3(){
		this("white", "auto", 4);
	}
	
	Car3(Car c){
		color = c.color;
		gearType = c.gearType;
		door = c.door;
	}
	Car3(String color, String gearType, int door){
		this.color = color;
		this.gearType = gearType;
		this.door = door;
	}
	
	
}