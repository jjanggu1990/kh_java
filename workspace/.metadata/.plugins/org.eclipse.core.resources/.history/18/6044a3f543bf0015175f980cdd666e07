package java_0120;

class _001IfEx {

	public static void main(String[] args) {
		/*
		 * args : 인자, 인수, parameter, arguments
		 * 전달되는 값
		 */
		
		int a = 100;
		/*
		 * str 은 초기화가 안되 있음.
		 * 하지만 밑에 if와 else 로써 조건문에 상관없이
		 * 초기화 해주므로 아래 식은 오류없이 실행됨 
		 * 
		 */
	
		String str;
		
		if(a >= 50){
			str = "50이상";
		}else {
			str = "50미만";
		}
		
		System.out.println(str);
	}

}
