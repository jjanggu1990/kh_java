package java_0122;

class _003CarTest {

	public static void main(String[] args) {
		Car c1 = new Car();
		c1.color = "white";
		c1.gearType = "auto";
		c1.door = 4;
	
		Car2  c2 = new Car("White", "auto", 4);
		System.out.println("c1 ÀÇ color = "+ c1.color + ", gearType = "+ c1.gearType + ", door = "+c1.door););
		

	}

}
class Car{
	String color;
	String gearType;
	int door;
	Car(){
		
	}
	Car(String c, String g, int d){
		color = c;
		gearType = g;
		door = d;
		
	}
}