package java_0215;

import java.io.FileReader;
import java.io.IOException;
import java.util.Scanner;

class ScannerReadableEx {

	public static void main(String[] args) {
		Scanner scan = null;
		FileReader fr = null;
		try{
			fr = new FileReader("f://scan.txt");
		}catch (IOException ioe){
			ioe.printStackTrace();
		}

	}

}
