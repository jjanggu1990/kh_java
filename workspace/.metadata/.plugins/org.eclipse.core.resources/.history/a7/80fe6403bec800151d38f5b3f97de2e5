package java_0201;

class CloneEx1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}

class Point implements Cloneable{
	int x ,y;
	Point (int x, int y ){
		this.x=x;
		this.y=y;
	}
	
	public String toString(){
		return "x ="+x+", y ="+y;
	}
	public Point clone(){
		Object obj = null;
		try{
			obj = super.clone();
		}catch(CloneNotSupportedException e){}
		return (Point)obj;
	}
}