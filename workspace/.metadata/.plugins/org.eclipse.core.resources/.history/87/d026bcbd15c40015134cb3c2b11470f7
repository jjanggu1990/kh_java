class Product1{
	int price;
	int bonusPoint;
	
	Product1(int price){
		this.price = price;
		bonusPoint = (int)(price/10.0);
	}
	Product1(){
		price = 0;
		bonusPoint=0;
		
	}
}


class Tv1 extends Product1{
	Tv1(){
		super(100);
	}
	
	public String toString(){
		return "Tv";
	}
}

class Computer1 extends Product1{
	Computer1(){
		super(200);
	}
	
	public String toString(){
		return "Computer";
	}
}

class Audio1 extends Product1{
	Audio1(){
		super(50);
	}
	
	public String toString(){
		return "Audio";
	}
}

class Buyer1{
