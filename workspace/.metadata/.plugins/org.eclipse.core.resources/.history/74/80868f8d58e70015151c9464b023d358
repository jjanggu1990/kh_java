<script>

	//배열은 모든 형태의 값을 저장할 수 있다.
	//존재하지 않는 인덱스는 모두 undefined로 출력한다.
	var array= [123, "String", true, function(){},{},[345,112]];
	document.write(array[100]);
	document.write('<br>');
	document.write(array.length);
	
	var arr5 = new Array();
	var arr6 = new Array(1,2,3,4);
	for(var i = 0; i<array.length; i++){
		document.write(array[i]+'<br>');
	}
	
	for(var i in array){
		document.write(i+'     ');
		document.write(array[i] + '<br>');
	}
</script>


