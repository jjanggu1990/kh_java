package java_0201;

class EqualsEx2 {

	public static void main(String[] args) {
		Person p1 = new Person(6516564551);
		Person p2 = new Person(6516664551);
	}

}


class Person{
	long id ;
	Person(long id){
		this.id = id;
	}
	
	public boolean equals(Object obj){
		if(obj != null && obj instanceof Person){
			return id == ((Person)obj).id;
		}
		return false;
	}
}