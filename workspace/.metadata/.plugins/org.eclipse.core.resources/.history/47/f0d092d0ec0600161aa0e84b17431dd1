<script>
	function checkIt(){
		var userinput = eval("document.userinput");
		if(!userinput.id.value){
			alert("ID 를 입력하세요.");
			return false;
		}
		if(!userinput.passwd.value){
			alert("비밀번호를 입력하세요.");
			return false;
		}
		return true;
	}
</script>
</head>
<body>
