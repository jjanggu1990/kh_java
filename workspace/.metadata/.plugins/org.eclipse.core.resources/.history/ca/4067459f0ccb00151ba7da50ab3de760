package java_0204;

import java.util.HashMap;

class MapEx1 {

	public static void main(String[] args) {
		String [] msg = {"Berlin", "Dortmund", " Frankfurt","Gelsenkirchen","Hamburg"};
		
		HashMap <Integer, String> map = new HashMap<Integer, String>();
		
		for (int i =0; i<msg.length;i++){
			map.put(i, msg[i]);
		}

	}

}
