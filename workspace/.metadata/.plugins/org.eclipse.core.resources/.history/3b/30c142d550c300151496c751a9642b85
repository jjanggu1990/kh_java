class Time{
	private int hour;
	private int minute;
	private int second;
	
	Time(int hour, int minute, int second){
		
	}
	
	public void setHour(int hour){
		if(hour<0 || hour >24) return;
		this.hour = hour;
	}
	public void setMinute(int minute){
		if(minute<0 || minute >60) return;
		this.minute = minute;
	}
	public void setSecond(int second){
		
	}
	
	
}