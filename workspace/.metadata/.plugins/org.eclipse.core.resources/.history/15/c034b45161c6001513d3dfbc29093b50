package java_0129;

class ExceptionEx {

	public static void main(String[] args) {
		int[] i = { 10, 20, 30 };
		
		//ArrayIndexOutOfBoundsException ¹ß»ý!!
		for (int j = 0; j <= i.length; j++) {
			try{
				System.out.println(i[j]);
			}catch(ArrayIndexOutOfBoundsException e){
				e.printStackTrace();
			}
		}
		System.out.println("program END");

	}

}
