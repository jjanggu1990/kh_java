package java_0127;

class VarTest {
	void argTest(int a,String...n){
		for(int i = 0; i< n.length; i++){
			System.out.print(n[i]+" ");
		}
		System.out.println();
	}

	public static void main(String[] args) {
		VarTest vt = new VarTest();
		
		vt.argTest("a","b");
		
		vt.argTest("a","b","c","d");

	}

}
