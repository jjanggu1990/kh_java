package java_0204;

import java.util.Random;

class RandomEx1 {

	public static void main(String[] args) {
		String [] lesson = {"JAVA Beg", "JSP", "XML&JAVA", "EJB"};
		
		for(int i =0; i<100; i++){
			Random r1 = new Random(3);
		int index = r1.nextInt(4);
		System.out.println("선택과목 : " + lesson[index]);
		}
	}

}
