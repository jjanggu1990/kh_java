class Product{
	int price;
	int bonuspoint;
	
	Product(int price){
		this.price = price;
		bonuspoint = (int)(price/10.0);
	}
}

class Tv extends Product{
	Tv(){
		super(100);
	}
	public String toString(){
		return "Tv";
	}
}

class Computer extends Product{
	Computer(){
		super(200);
	}
	
	public String toString(){
		return "Computer";
	}
}

class Buyer{
	int monet = 1000;
	int bonuspoint = 0;
	
	void buy(Product p){
		if(money< p.price){
			System.out.println("ÀÜ¾×ºÎÁ·");
			return;
		}
	}
}