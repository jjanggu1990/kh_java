package board;

import java.sql.*;

public class JdbcUtil {
	public static void close(PreparedStatement pstmt){
		if(pstmt !=null){
			try{
				pstmt.close();
			}catch(Exception e){
			}

		}
	}
	public static void close(Connection conn){
		if(conn !=null){
			try{
				conn.close();
			}catch(Exception e){
			}

		}
	}
	public static void close(PreparedStatement pstmt){
		if(pstmt !=null){
			try{
				pstmt.close();
			}catch(Exception e){
			}

		}
	}
	
}
