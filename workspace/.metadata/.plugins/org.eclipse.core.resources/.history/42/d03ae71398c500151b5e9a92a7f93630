package java_0128;

class Test {

	public static void method(T v1, T v2){
		v2.x = 20;
		v1 = v2;
	}
	public static void main(String [] args){
		T x1 = new T (10);
		T x2 = new T (10);
		
		method(x1,x2);
		System.out.println(x1.x  + "    " + x2.x);
	}

}



class T{
	int x ;
	public T(int x){
		this.x = x;
	}
}